<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true">
  <class name="flymetomars.dataaccess.entities.LocationEntity" table="location">
    <id column="location_id" name="id">
      <generator class="native"/>
    </id>
    <property length="12" name="floor"/>
    <property length="10" name="streetNo"/>
    <property length="40" name="street" not-null="true"/>
    <property length="30" name="landmark"/>
    <property length="30" name="town" not-null="true"/>
    <property length="30" name="region" not-null="true"/>
    <property length="12" name="postcode"/>
    <property length="32" name="country" not-null="true"/>
  </class>
</hibernate-mapping>
